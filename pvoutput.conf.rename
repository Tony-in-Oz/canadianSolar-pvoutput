# Set your timezone string so pytz can grab your localtime inside docker
# defaults to 'Etc/UTC'
timezone='America/Sao_Paulo'

[pvoutput]
# enter a comma separated list of systems, one for each inverter.
systemID='12345', '09876'
APIKEY='abcde123456789'

[inverters]
# enter a comma separated list of addresses, defaults to 0x1
#addresses=0x1,0x2,0xa2
#port='/dev/ttyUSB0'

[owm]
# optional section
#OWMKEY='abcde123456789'
#latitude=-22.33
#longitude=49.75